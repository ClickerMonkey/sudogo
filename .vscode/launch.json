  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch test function",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}/pkg",
      "args": [
        "-test.run",
        "TestGenerateClear"
      ]
    },
    {
      "name": "Launch test",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}/pkg",
      "args": []
    },
    {
      "name": "Launch generate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/gen",
      "args": [
        "-boxWidth=3",
        "-boxHeight=2",
        "-type=custom",
        "-maxPlacements=18"
      ]
    },
    {
      "name": "Launch rest",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/rest"
    },
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    }
  ]
}